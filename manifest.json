{
    "manifest_version": 3,
    "name": "Bookmark Leads",
    "description": "Save your leads form Linkedin, Facebook or anywhere. once you add it. it won't go away until you delete it",
    "version": "1.0",
    "icons": {
        "16": "images/bookmark-leads32.png",
        "32": "images/bookmark-leads32.png",
        "64": "images/bookmark-leads32.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "https://developer.chrome.com/*"
    ],
    "permissions": [
        "tabGroups",
        "tabs"
    ]
}